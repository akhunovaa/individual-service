<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd"
    updateCheck="true" monitoring="autodetect" dynamicConfig="true">

    <diskStore path="java.io.tmpdir" />

    <cache name="individual" maxEntriesLocalHeap="10000" maxEntriesLocalDisk="1000" eternal="false"
        diskSpoolBufferSizeMB="50" timeToIdleSeconds="600" timeToLiveSeconds="1600" memoryStoreEvictionPolicy="LFU"
        transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

    <cache name="user" maxEntriesLocalHeap="10000" maxEntriesLocalDisk="1000" eternal="false" diskSpoolBufferSizeMB="50"
        timeToIdleSeconds="600" timeToLiveSeconds="1600" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

    <cache name="bytesFromPersistentVolume" maxEntriesLocalHeap="1000" maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="50"
        timeToIdleSeconds="600" timeToLiveSeconds="1600" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

    <cache name="imagePath" maxEntriesLocalHeap="1000" maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="50"
        timeToIdleSeconds="600" timeToLiveSeconds="1600" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

    <cache name="user-info-data" maxEntriesLocalHeap="1000" maxEntriesLocalDisk="1000" eternal="false"
        diskSpoolBufferSizeMB="100" timeToIdleSeconds="120" timeToLiveSeconds="300" memoryStoreEvictionPolicy="LFU"
        transactionalMode="off">
        <persistence strategy="localTempSwap" />
    </cache>

</ehcache>